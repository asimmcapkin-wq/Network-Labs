configure terminal
hostname L2Sw3

! VLAN Tanımlaması
vlan 40
 name Yonetim_Vlan
exit

! CoreSW'ye Giden Uplink Portu (Trunk)
interface range Ethernet0/3, Ethernet0/0
 switchport trunk encapsulation dot1q
 switchport mode trunk
exit

! Kullanıcı (PC) Portları (Access)
interface range Ethernet0/1 - 2 , Ethernet1/1-2
 switchport mode access
 switchport access vlan 40
exit

end
write memory
